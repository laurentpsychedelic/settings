# -*- mode: snippet -*-
# name: stringarray
# key: stringarray
# type: command
# --
(let ((yas-good-grace nil)
      (count (read-number "Number of elements: ")))
  (yas-expand-snippet
   (concat "String [] ${1:name} = new String [] { "
           (mapconcat
            (lambda (num)
              (format "\"${%d:ele%d}\"" (1+ num) num))
            (number-sequence 1 count nil)
            ", ")
           " };")))
