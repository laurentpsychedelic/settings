# -*- mode: snippet -*-
# name: enum
# key: enum
# type: command
# --
(let ((yas-good-grace nil)
      (count (read-number "Number of elements: ")))
  (yas-expand-snippet
   (concat "public enum ${1:name} {\n"
           "\t"
           (mapconcat
            (lambda (num)
              (format "${%d:ele%d}" (1+ num) num))
            (number-sequence 1 count nil)
            ", ")
           "\n"
           "}\n")))
